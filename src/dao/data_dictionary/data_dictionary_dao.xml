<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.data_dictionary.data_dictionary_dao">

	<select id="selectCpType" resultType="data_dictionary">
		select * from
		data_dictionary where typeName = "所属平台"
	</select>

	<select id="selectApptype" resultType="data_dictionary">
		select * from
		data_dictionary where typeName = "APP状态"
	</select>
</mapper>